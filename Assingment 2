#include <stdio.h>

int main() {
    int num1, num2, square1, square2;

   
    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);

    
    square1 = num1 * num1;
    square2 = num2 * num2;

   
    printf("Square of %d = %d\n", num1, square1);
    printf("Square of %d = %d\n", num2, square2);

    return 0;
}